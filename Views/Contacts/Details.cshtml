@model WebServer.Models.Contacts

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Contacts</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Contact List:
       </dt>
       <dd>
           <ul>
                @foreach (var user in Model.Users)
                {
                    <li>@user.Nickname</li>
                }
            </ul>
       </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Username">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
